#list {
	margin: 30px auto;
	width: 700px;
	
	h1 {
		float: left;
		font-family: $raleway;
		font-size: 54px;
		line-height: 1.2em;
		margin: 0 0 15px 0;
	}
	
	a {
		@include gradient(#f6f6f6, #eee);
		@include radius(4px);
		border: 1px solid #dbdbdb;
		color: #b8b8b8;
		cursor: pointer;
		float: right;
		margin: 8px 0 0 8px;
		padding: 10px 20px;
		text-shadow: 0 1px 1px #fff;
		
		&:hover {
			@include gradient(#df4c4b, #c43037);
			border: 1px solid #884f51;
			color: #7e171e;
			text-shadow: 0 1px 1px #e35851;
		}
		
		&:active {
			@include gradient(#c43037, #df4c4b);
			border: 1px solid #884f51;
			color: #7e171e;
			text-shadow: 0 1px 1px #e35851;
		}
	}
	
	table {
		background: #fff;
		border: 1px solid #d8d8d8;
		float: left;
		margin: 15px 0 0;
		width: 100%;
		
		thead {
			width: 100%;
			
			td {
				@include gradient($color, #2e5cc7);
				@include boxshadow(inset 0 1px 1px 0 #3c7aef);
				
				border: 1px solid #2d59b3;
				border: 1px solid #2d59b3;
				color: #f7f7f7;
				padding: 10px;
			}
		}
		tr {
			td {
				padding: 10px;
			}
			&.even {
				background: #eee;
			}
		}
	}
}
