		}
		
		li {
			color: #909090;
			display: block;
			float: left;
			font-size: 15px;
			margin: 0 0 10px 0;
			width: 100%;
			
			span {
				background: url('images/featureicons.jpg') no-repeat -32px 0;
				display: block;
				float: left;
				height: 31px;
				margin: 0 25px 0 0;
				width: 31px;
				
				&#feature1 { background-position: 0 0};
				&#feature2 { background-position: -31px 0};
				&#feature3 { background-position: -62px 0};
				&#feature4 { background-position: -93px 0};
				&#feature5 { background-position: -124px 0};
			}
		}
	}
	
	.right-50 {
		float: right;
		width: 390px;
		
		form { /* hompage form */
			input[type=text],
			input[type=password] {
				width: 354px;
			}
			
			input[type=submit] {
				@include buttontrio;
			}
			
			p {
				color: #ccc;
				float: left;
				font-size: 14px;
				margin: 15px 0 0 0;
			}
		}
	}
	
	.right-67 {
		float: right;
		width: 580px;
		
		ul {
			float: right;
			li {
				float: left;
			}
		}
		
		form {
			@include container;
			
			fieldset#terms {
				label {
					width: 100%;
					float: left;
					
					input {
						display: inline;
						float: left;
						margin: 0 0 0 160px;
					}
					
					p {
						float: left;
						margin: 0 0 0 15px;
					}
					
				}
			}
			
			label {
				float: left;
				font-size: 16px;
				height: 35px;
				line-height: 1.2em;
				padding: 18px 0 0;
				width: 160px;
				
				&.two-lines {
					padding: 9px 0 0;
				}
				
				&.error {					
					color: #df4c4b;
					float: left;
					margin: 0 0 15px 160px;
					padding: 0;
					width: 340px;
				}
				
				&.radio {
					float: left;
					height: 15px;
					width: 340px;
				}
			}
			
			input[type=text],
			input[type=password],
			select {
				width: 340px;
			}
			
			input[type=submit] {
				@include buttontrio;
				float: right;
			}
			
			select {
				height: 30px;
				margin: 18px 0 30px 0;
			}
			
			a {
				@extend .right-50;
			}
		}
		
		#form-login {
			@extend #form-signup;
			
			input[type=text],
			input[type=password] {
				width: 500px;
			}
		}
		
		article {
			@include container;
			margin: 0 0 20px 0;
			width: 540px;
			
			div {
				float: left;
				width: 483px;
			}
			
			p {
				float: left;
				margin: 0;
			}
			
			img {
				float: right;
			}
			
			span.tag {
				@include radius-right(4px);
				background: #e8e8e8;
				color: #bbb;
				display: block;
				float: right;
				font-size: 14px;
				margin: 0 0 0 25px;
				padding: 2px 10px;
				position: relative;
				text-transform: uppercase;
				
				&:before {
					border-color: transparent #e8e8e8 transparent transparent;
					border-style: solid;
					border-width: 14px 14px 14px 0;	
					content: "";
					float: left;
					height: 0;	
					left: -14px;
					position: absolute;
					top: 0;
					width: 0;
				}
				
				&:after{
					@include radius(4px);
					@include boxshadow(0 -1px 1px 0 #ffe - #333);
					content:"";
					position:absolute;
					top:10px;
					left:0;
					float:left;
					width:4px;
					height:4px;
					background:#fff;
				}
			}
		}
	}
} /* end #mid */

#bot {
	@include radius-bot(10px);
	color: #fff;
	float: left;
	height: 25px;
	width: 100%;
}

#form {
	float: right;
	width: 390px;
	
	h2 {
		font-family: $raleway;
		font-size: 54px;
		line-height: 1.2em;
	}

	p {
		color: #bbb;
		margin: 0 0 10px 0;
		text-shadow: 0 1px 1px #fff;
	}
}

