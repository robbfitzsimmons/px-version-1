#list {
	margin: 30px auto;
	width: 700px;
	
	h1 {
		float: left;
		font-family: $raleway;
		font-size: 54px;
		line-height: 1.2em;
		margin: 0 0 15px 0;
	}
	
	a {
		@include gradient(#f6f6f6, #eee);
		@include radius(4px);
		border: 1px solid #dbdbdb;
		color: #b8b8b8;
		cursor: pointer;
		float: right;
		margin: 8px 0 0 8px;
		padding: 10px 20px;
		text-shadow: 0 1px 1px #fff;
		
		&:hover {
