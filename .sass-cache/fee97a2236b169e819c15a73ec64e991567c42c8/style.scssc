				margin: 0 15px 0 0;
			}
			
			a {
				@include button(1);
			}
		}
		
		hr {
			color: #eaeaea;
			background: #eaeaea;
			float: left;
			height: 1px;
			margin: 60px 0;
			width: 100%;
		}
		
		form {
			margin: 0 auto;
			width: 530px;
			
			input[type=text],
			input[type=password] {
				width: 494px;
			}
		}
	} /* landing */
	
	p {
		margin: 0 0 20px 0;
	}
	
	a {
		text-decoration: underline;
		
		&:hover {
			text-decoration: none;
		}
		
		&.button {
			@include button(150px);
			float: right;
			margin: 0 0 20px 0;
		}
	}
	
	form {
	
		fieldset.radio {
			margin: 0 0 25px 0;
		}
		
		input[type=text],
		input[type=password] {
			@include radius(4px);
			@include boxshadow2(inset 0 2px 2px 0 #f7f7f7, 0 0 0 0 #fff);
			border: 1px solid #dedede;
			color: #bbb;
			height: 16px;
			line-height: 1em;
			margin: 0 0 20px 0;
			padding: 18px;
			
			&.error {
				@include boxshadow(0 0 4px 0 #862020);
				background: #f0b7b7;
				border-color: #dc8181;
				color: #bf4e4d;
				
				&:hover {
					background: #f0b7b7;
				}
			}
		}
		
		input[type=radio] {
			margin: 0 10px 0 0;
		}
	
		input[type=submit] {
			@include button(110px);
			float: right;
		}
		
		textarea {
			@extend input[type=text];
			height: 46px;
		}
	}
	
	.left-33 {
		float: left;
		width: 245px;
		
		ul {
			float: left;
			width: 100%;
			
			a {
				

				&.filtered {
					color: #909090;
					cursor: text;
					font-weight: bold;
					text-decoration: none;
				}
			}
		}
		
		h4 {
			float: left;
			width: 100%;
		}
		
		p {
			float: left;
			width: 100%;
			img {
				float: left;
				margin: 0 10px 0 0;
				width: 25px;
			}
		}
			
	}
	
	.left-50 {
		float: left;
		width: 390px;
		
		h2 {
			color: #484848;
			font-family: $raleway;
			font-size: 44px;
			line-height: 1em;
			margin: 0 0 45px 0;
		}
		
		li {
			color: #909090;
			display: block;
			float: left;
			font-size: 15px;
			margin: 0 0 10px 0;
			width: 100%;
			
			span {
				background: url('images/featureicons.jpg') no-repeat -32px 0;
				display: block;
				float: left;
				height: 31px;
				margin: 0 25px 0 0;
				width: 31px;
				
				&#feature1 { background-position: 0 0};
				&#feature2 { background-position: -31px 0};
				&#feature3 { background-position: -62px 0};
				&#feature4 { background-position: -93px 0};
				&#feature5 { background-position: -124px 0};
			}
		}
	}
	
	.right-50 {
		float: right;
		width: 390px;
		
		form { /* hompage form */
			input[type=text],
			input[type=password] {
				width: 354px;
			}
			
			input[type=submit] {
				@include buttontrio;
			}
			
			p {
				color: #ccc;
				float: left;
				font-size: 14px;
				margin: 15px 0 0 0;
			}
		}
	}
	
	.right-67 {
		float: right;
		width: 580px;
		
		ul {
			float: right;
			li {
				float: left;
			}
		}
		
		form {
			@include container;
			
			fieldset#terms {
				label {
					width: 100%;
					float: left;
					
					input {
						display: inline;
						float: left;
						margin: 0 0 0 160px;
					}
					
					p {
						float: left;
						margin: 0 0 0 15px;
					}
					
				}
			}
			
			label {
				float: left;
				font-size: 16px;
				height: 35px;
				line-height: 1.2em;
				padding: 18px 0 0;
				width: 160px;
				
				&.two-lines {
					padding: 9px 0 0;
				}
				
				&.error {					
					color: #df4c4b;
					float: left;
					margin: 0 0 15px 160px;
					padding: 0;
					width: 340px;
				}
				
				&.radio {
					float: left;
					height: 15px;
					width: 340px;
				}
			}
			
			input[type=text],
			input[type=password],
			select {
				width: 340px;
			}
			
			input[type=submit] {
				@include buttontrio;
				float: right;
			}
			
			select {
				height: 30px;
				margin: 18px 0 30px 0;
			}
			
			a {
				@extend .right-50;
			}
		}
		
		#form-login {
			@extend #form-signup;
			
			input[type=text],
			input[type=password] {
				width: 500px;
			}
		}
		
		article {
			@include container;
			margin: 0 0 20px 0;
			width: 540px;
			
			div {
				float: left;
				width: 483px;
			}
			
			p {
				float: left;
				margin: 0;
			}
			
			img {
				float: right;
			}
			
			span.tag {
				@include radius-right(4px);
				background: #e8e8e8;
				color: #bbb;
				display: block;
				float: right;
				font-size: 14px;
				margin: 0 0 0 25px;
				padding: 2px 10px;
				position: relative;
				text-transform: uppercase;
				
				&:before {
					border-color: transparent #e8e8e8 transparent transparent;
					border-style: solid;
					border-width: 14px 14px 14px 0;	
					content: "";
					float: left;
					height: 0;	
					left: -14px;
					position: absolute;
					top: 0;
					width: 0;
				}
				
				&:after{
					@include radius(4px);
					@include boxshadow(0 -1px 1px 0 #ffe - #333);
					content:"";
					position:absolute;
					top:10px;
					left:0;
					float:left;
					width:4px;
					height:4px;
					background:#fff;
				}
			}
		}
	}
} /* end .mid */

.bot {
	@include radius-bot(10px);
	color: #fff;
	float: left;
