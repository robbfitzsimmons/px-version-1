			color: #7e171e;
			text-shadow: 0 1px 1px #e35851;
		}
		
		&:active {
			@include gradient(#c43037, #df4c4b);
			border: 1px solid #884f51;
			color: #7e171e;
			text-shadow: 0 1px 1px #e35851;
		}
	}
	
	table {
		background: #fff;
		border: 1px solid #d8d8d8;
		float: left;
		margin: 15px 0 0;
		width: 100%;
		
		thead {
			width: 100%;
			
			td {
