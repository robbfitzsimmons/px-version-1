table.jCalendar {
	th {
		@include gradient(#eff2f5, #e0e6ec);
		border-bottom: 1px solid #ccc;
		color: #909090;
		font-size: 11px;
		padding: 5px 7px;
	}
	
	td {
		@include boxshadow(inset 0 1px 1px 0 #fff);
		background: #f5f5f5;
		border: 1px solid #e7e9ea;
		padding: 5px 7px;
		text-align: center;
			
		&.other-month {
			color: #c4c4c4;
		}
		&.today {
			@include boxshadow(none);
			background: #ccc;
			color: #fff;
			
			&:hover {
				background: #777;
				color: #fff;
			
			}
		}
		&.selected {
			@include boxshadow(none);
			background: #df4c4b;
			color: #fff;
		}
		&.selected.dp-hover {
			color: #fff;
		}
		
		&.disabled,
		&.disabled.dp-hover,
		&.unselectable,
		&.unselectable:hover,
		&.unselectable.dp-hover {
			color: #e4e4e4;
			text-decoration: line-through;
			&:hover {
				background: #f5f5f5;
			}
		}
		td.dp-hover,
		tr.activeWeekHover td {
			background: #fff;
			color: #000;
		}
		tr.selectedWeek td {
			background: #f66;
			color: #fff;
		}
	}
}
	
	div {
	
		&.dp-popup {
			@include boxshadow(0 0 20px 0 #ccc);
			font-size: 14px;
			left: 910px;
			line-height: 1.2em;
			position: relative;
			width: 217px;
			
			h2 {
				@include radius-top(4px);
				@include gradient($color, $color - #111);
				@include boxshadow(inset 0 1px 1px 0 $color + #222);
				border: 1px solid $color - #222;
				color: #eee;
				font-size: 16px;
				margin: 0;
				padding: 3px 0;
				text-align: center;
			}
			
			a {
				color: #000;
				text-decoration: none;
				padding: 3px 2px 0;
			}
			
			.dp-nav-prev-year,
			.dp-nav-next-year {
				display: none;
			}
		}
		
		&#dp-popup {
			position: absolute;
			z-index: 199;
		
		}
	}
	
	a#dp-close {
		font-size: 11px;
		padding: 4px 0;
		text-align: center;
		display: block;
		
		&:hover {
			text-decoration: underline;
		}
	}
	div.dp-popup div.dp-nav-prev {
		position: absolute;
		top: 2px;
		left: 4px;
		width: 100px;
		
		a {
			float: left;
		}
	}
	/* Opera needs the rules to be this specific otherwise it doesn't change the cursor back to pointer after you have disabled and re-enabled a link */
	div.dp-popup div.dp-nav-prev a, div.dp-popup div.dp-nav-next a {
		cursor: pointer;
	}
	div.dp-popup div.dp-nav-prev a.disabled, div.dp-popup div.dp-nav-next a.disabled {
		cursor: default;
	}
	div.dp-popup div.dp-nav-next {
		position: absolute;
		top: 2px;
		right: 4px;
		width: 100px;
	}
	div.dp-popup div.dp-nav-next a {
		float: right;
	}
	div.dp-popup a.disabled {
		cursor: default;
	}
	div.dp-popup td {
		cursor: pointer;
	}
	div.dp-popup td.disabled {
		cursor: default;
	}

/* end date picker */

