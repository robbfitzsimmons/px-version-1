.reveal-modal-bg { 
	position: fixed; 
	height: 100%;
	width: 100%;
	background: #000;
	background: rgba(0,0,0,.5);
	z-index: 100;
	display: none;
	top: 0;
	left: 0; 
}

.reveal-modal {
	visibility: hidden;
	top: 100px; 
	left: 50%;
	min-height: 5px;
	margin: 0 0 -20px -300px;
	width: 579px;
	background: #eee /* url(modal-gloss.png) no-repeat -200px -80px */;
	position: absolute;
	z-index: 101;
	padding: 0;
	@include radius(10px);
	@include boxshadow(0 0 10px rgba(0,0,0,.4));
}
	
.reveal-modal.small 		{ width: 200px; margin-left: -140px;}
.reveal-modal.medium 		{ width: 400px; margin-left: -240px;}
.reveal-modal.large 		{ width: 600px; margin-left: -340px;}
.reveal-modal.xlarge 		{ width: 800px; margin-left: -440px;}

.reveal-modal .close-reveal-modal {
	font-size: 22px;
	line-height: .5;
	position: absolute;
	top: 8px;
	right: 11px;
	color: #aaa;
	text-shadow: 0 -1px 1px rbga(0,0,0,.6);
	font-weight: bold;
	cursor: pointer;
	} 
/* end modal windows */


.flash {
	bottom: 0;
	display: none;
	padding: 20px 0;
	position: fixed;
	text-align: center;
	width: 100%;
	z-index: 10;
	
	&#error {
		$errorcolor: #c43037;
		@include boxshadow(hilite 0 2px 2px 0 $errorcolor + #666);
		@include gradient($errorcolor + #444, $errorcolor + #333);
		border-top: 1px solid $errorcolor;
		color: $errorcolor;
	}
	&#success {
		$successcolor: #2e9716;
		@include boxshadow(hilite 0 2px 2px 0 $successcolor + #666);
		@include gradient($successcolor + #444, $successcolor + #333);
		border-top: 1px solid $successcolor;
		color: $successcolor;
	}
	&#warning {
		$successcolor: #fecb00;
		@include boxshadow(hilite 0 2px 2px 0 $successcolor + #666);
		@include gradient($successcolor + #111, $successcolor + #222);
		border-top: 1px solid $successcolor - #222;
		color: $successcolor - #333;
	}
}

::selection {
