	}
	
	em {
		font-style: italic;
	}
}

.container {
	margin: 0 auto;
	width: 960px;
	
	@include color(#3870d1, #ecf0f8);
	
	&.dark-gray { @include color(#777, #e8e8e8); }
	&.dark-gray-blue { @include color(#30627c, #dfe8ed); }
	&.forest-green { @include color(#016367, #e3f6ea); }
