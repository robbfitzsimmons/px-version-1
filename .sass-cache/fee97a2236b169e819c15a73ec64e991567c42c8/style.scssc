	div {
	
		&.dp-popup {
			@include boxshadow(0 0 20px 0 #ccc);
			font-size: 14px;
			left: 910px;
			line-height: 1.2em;
			position: relative;
			width: 217px;
			
			h2 {
				@include radius-top(4px);
