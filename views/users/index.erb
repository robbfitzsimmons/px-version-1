    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.password %></td>
    <td><%= user.salt %></td>
    <td><%= user.linked_in %></td>
    <td><%= user.facebook %></td>
    <td><%= user.twitter %></td>
    <td><%= user.website %></td>
    <td><%= user.image %></td>
    <td><%= user.location %></td>
    <td><%= user.created_at %></td>
    <td><%= user.updated_at %></td>
